<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox>
               <children>
                  <GridPane>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Button fx:id="buttonEjecutar" alignment="CENTER" mnemonicParsing="false" text="Run" />
                        <Button fx:id="buttonReiniciar" alignment="CENTER" mnemonicParsing="false" text="Reset" GridPane.columnIndex="1" />
                        <Button fx:id="buttonGuardar" alignment="CENTER" mnemonicParsing="false" text="Save" GridPane.columnIndex="2" />
                     </children>
                  </GridPane>
               </children>
            </HBox>
            <HBox>
               <children>
                  <ListView fx:id="listAlgoritmo" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Label fx:id="labelInformacion" />
         </children>
      </VBox>
   </children>
</AnchorPane>
